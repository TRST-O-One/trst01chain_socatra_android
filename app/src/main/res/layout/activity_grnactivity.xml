<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <!--    header-->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_35sdp"
        android:background="@color/Green">

        <ImageView
            android:id="@+id/ccx_logo_white"
            android:layout_width="89dp"
            android:layout_height="32dp"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginStart="6dp"
            android:layout_marginTop="6dp"
            android:layout_marginEnd="6dp"
            android:layout_marginBottom="6dp"

            android:padding="2dp"
            android:src="@drawable/logotrack"
            tools:ignore="RtlCompat" />


    </RelativeLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:background="@color/white"
        android:orientation="horizontal">

        <ImageView

            android:id="@+id/image_back_sc"
            android:layout_width="14dp"
            android:layout_height="@dimen/_24sdp"
            android:layout_weight="0.3"
            android:src="@drawable/back"
            app:tint="@color/Green" />


        <TextView
            style="@style/CustomTextViewPadding5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="20dp"
            android:layout_weight="0.7"
            android:id="@+id/txt_word_grn_header"
            android:fontFamily="@font/montserrat_medium"
            android:gravity="right"
            android:text="@string/grn_details"
            android:textColor="@color/black"
            android:textSize="@dimen/_14ssp"
            android:textStyle="bold" />


    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="horizontal">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="13sp"
            android:layout_marginEnd="13sp"
            android:orientation="vertical">

            <!--Dealer-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txt_word_dealer"
                    style="@style/CustomTextViewPadding5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1.4"
                    android:fontFamily="@font/montserrat_regular"
                    android:text="@string/dealer"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_10ssp" />

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_2sdp"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_weight="1"
                    app:cardBackgroundColor="@color/grey"
                    app:cardCornerRadius="@dimen/_3sdp"
                    app:cardElevation="@dimen/_2sdp">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/border"></FrameLayout>

                    <com.google.android.material.textfield.TextInputEditText

                        android:id="@+id/et_dealer_name"

                        style="@style/CustomEditTextPadding5dp"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_25ssp"
                        android:background="@null"
                        android:backgroundTint="@android:color/transparent"
                        android:enabled="false"
                        android:fontFamily="@font/montserrat_regular"
                        android:inputType="number"
                        android:paddingStart="@dimen/_6ssp"
                        android:textColor="@color/enabled_color"
                        android:textCursorDrawable="@null"

                        android:textSize="@dimen/_10ssp" />

                </androidx.cardview.widget.CardView>

            </LinearLayout>


            <!--select type-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:orientation="horizontal"
                android:visibility="gone">

                <TextView
                    android:id="@+id/txt_selecttype_name"
                    style="@style/CustomTextViewPadding5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1.4"
                    android:fontFamily="@font/montserrat_regular"
                    android:text="Select Type"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_10ssp" />

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_2sdp"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_weight="1"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="@dimen/_3sdp"
                    app:cardElevation="@dimen/_2sdp">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/border"></FrameLayout>

                    <androidx.appcompat.widget.AppCompatSpinner
                        android:id="@+id/sp_select_type_new"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_25ssp"
                        android:fontFamily="montserrat_regular"
                        android:visibility="visible" />

                </androidx.cardview.widget.CardView>

            </LinearLayout>


            <!--Farmer-->
            <LinearLayout
                android:id="@+id/ll_select_farmer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:orientation="horizontal"
                android:visibility="visible">

                <TextView
                    android:id="@+id/txt_word_Farmer"
                    style="@style/CustomTextViewPadding5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1.4"
                    android:fontFamily="@font/montserrat_regular"
                    android:text="@string/farmer"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_10ssp" />
                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_2sdp"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_weight="1"
                    android:id="@+id/cd_select_farmer"

                    app:cardCornerRadius="@dimen/_3sdp"
                    app:cardElevation="@dimen/_2sdp">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/border">

                    </FrameLayout>

                    <TextView
                        android:id="@+id/txt_select_farmer"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_25ssp"
                        android:text="@string/select_farmer"
                        android:gravity="center_vertical"
                        android:textSize="@dimen/_10ssp"
                        android:layout_marginStart="@dimen/_4ssp"
                        android:drawableEnd="@drawable/baseline_arrow_drop_down_24"
                        android:textColor="@color/black"
                        android:visibility="visible" />

                    <TextView
                        android:id="@+id/txt_no_data"
                        style="@style/CustomEditTextPadding5dp"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_25ssp"
                        android:background="@color/grey"
                        android:enabled="false"
                        android:visibility="gone"
                        android:text="No Farmers Found"
                        android:backgroundTint="@android:color/transparent"
                        android:fontFamily="@font/montserrat_regular"
                        android:paddingStart="@dimen/_6ssp"
                        android:textColor="@color/enabled_color"
                        android:textCursorDrawable="@null"
                        android:textSize="@dimen/_10ssp" />


                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_2sdp"
                    android:visibility="gone"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_weight="1"

                    app:cardCornerRadius="@dimen/_3sdp"
                    app:cardElevation="@dimen/_2sdp">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/border">

                    </FrameLayout>

                    <androidx.appcompat.widget.AppCompatSpinner
                        android:id="@+id/sp_select_farmer"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_25ssp"
                        android:fontFamily="montserrat_regular"
                        android:visibility="visible" />

                </androidx.cardview.widget.CardView>
            </LinearLayout>

            <!--Sub dealer-->
            <LinearLayout
                android:id="@+id/ll_sub_dealer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:orientation="horizontal"
                android:visibility="gone">

                <TextView
                    android:id="@+id/subdealer"
                    style="@style/CustomTextViewPadding5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1.4"
                    android:fontFamily="@font/montserrat_regular"
                    android:text="Sub Dealer"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_10ssp" />

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_2sdp"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_weight="1"
                    app:cardBackgroundColor="@color/grey"
                    app:cardCornerRadius="@dimen/_3sdp"
                    app:cardElevation="@dimen/_2sdp">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/border"></FrameLayout>

                    <androidx.appcompat.widget.AppCompatSpinner
                        android:id="@+id/sp_select_sub_dealer"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_25ssp"
                        android:fontFamily="montserrat_regular"
                        android:visibility="visible" />

                </androidx.cardview.widget.CardView>
            </LinearLayout>

            <!--Invoice-->
            <LinearLayout
                android:id="@+id/ll_Invoice_dealer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:orientation="horizontal"
                android:visibility="gone">

                <TextView
                    android:id="@+id/Invoice"
                    style="@style/CustomTextViewPadding5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1.4"
                    android:fontFamily="@font/montserrat_regular"
                    android:text="Invoice"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_10ssp" />

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_2sdp"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_weight="1"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="@dimen/_3sdp"
                    app:cardElevation="@dimen/_2sdp">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/border"></FrameLayout>

                    <androidx.appcompat.widget.AppCompatSpinner
                        android:id="@+id/sp_select_Invoice"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_25ssp"
                        android:fontFamily="montserrat_regular"
                        android:visibility="visible" />

                </androidx.cardview.widget.CardView>
            </LinearLayout>


            <!--Quantity-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txt_word_quantity"
                    style="@style/CustomTextViewPadding5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1.4"
                    android:fontFamily="@font/montserrat_regular"
                    android:text="@string/quantity"
                    android:textAllCaps="false"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_10ssp" />

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_2sdp"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_weight="1"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="@dimen/_3sdp"
                    app:cardElevation="@dimen/_2sdp">


                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/border"></FrameLayout>

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/et_quantity_add"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_25ssp"
                        android:background="@null"
                        android:backgroundTint="@android:color/transparent"
                        android:fontFamily="@font/montserrat_regular"
                        android:inputType="number"
                        android:paddingStart="@dimen/_6ssp"
                        android:textColor="@color/enabled_color"
                        android:textCursorDrawable="@null"
                        android:textSize="@dimen/_10ssp" />

                </androidx.cardview.widget.CardView>


            </LinearLayout>

            <!--DRC-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:orientation="horizontal"
                android:visibility="gone">

                <TextView
                    android:id="@+id/txt_drc"
                    style="@style/CustomTextViewPadding5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1.4"
                    android:fontFamily="@font/montserrat_regular"
                    android:text="DRC"
                    android:textAllCaps="false"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_10ssp" />

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_2sdp"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_weight="1"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="@dimen/_3sdp"
                    app:cardElevation="@dimen/_2sdp">


                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/border"></FrameLayout>

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/et_drc_add"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_25ssp"
                        android:background="@null"
                        android:backgroundTint="@android:color/transparent"
                        android:fontFamily="@font/montserrat_regular"
                        android:inputType="text"
                        android:paddingStart="@dimen/_6ssp"
                        android:textColor="@color/enabled_color"
                        android:textCursorDrawable="@null"
                        android:textSize="@dimen/_10ssp" />

                </androidx.cardview.widget.CardView>


            </LinearLayout>

            <!--Mode of transport-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:orientation="horizontal"
                android:visibility="visible">

                <TextView
                    android:id="@+id/txt_word_mode_of_transport"
                    style="@style/CustomTextViewPadding5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1.4"
                    android:fontFamily="@font/montserrat_regular"
                    android:text="@string/mode_of_transport"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_10ssp" />

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_2sdp"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_weight="1"
                    app:cardCornerRadius="@dimen/_3sdp"
                    app:cardElevation="@dimen/_2sdp">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/border"></FrameLayout>

                    <androidx.appcompat.widget.AppCompatSpinner
                        android:id="@+id/sp_mode_of_transport_add"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_25ssp"
                        android:fontFamily="montserrat_regular"
                        android:visibility="visible" />

                </androidx.cardview.widget.CardView>
            </LinearLayout>


            <!--Date-->
            <LinearLayout
                android:id="@+id/layout_no_of_children"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txt_word_date"
                    style="@style/CustomTextViewPadding5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1.4"
                    android:fontFamily="@font/montserrat_regular"
                    android:text="@string/date"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_10ssp" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/cd_date"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_2sdp"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_weight="1"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="@dimen/_3sdp"
                    app:cardElevation="@dimen/_2sdp">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/border"></FrameLayout>

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/et_date_add"
                        style="@style/CustomEditTextPadding5dp"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_25ssp"
                        android:background="@null"
                        android:backgroundTint="@android:color/transparent"
                        android:enabled="false"
                        android:fontFamily="@font/montserrat_regular"
                        android:inputType="number"
                        android:paddingStart="@dimen/_6ssp"
                        android:text="srikanth"
                        android:textColor="@color/enabled_color"
                        android:textCursorDrawable="@null"
                        android:textSize="@dimen/_10ssp" />

                    <ImageButton
                        android:id="@+id/btn_open_date_picker"
                        android:layout_width="@dimen/_20ssp"
                        android:layout_height="@dimen/_20ssp"
                        android:layout_gravity="center_vertical|end"
                        android:background="?attr/selectableItemBackground"
                        android:contentDescription="Open Date Picker"
                        android:padding="@dimen/_8sdp"
                        android:src="@drawable/ic_calendar" />

                </androidx.cardview.widget.CardView>

            </LinearLayout>

            <!--GRN Evidence-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:orientation="horizontal"
                android:visibility="gone">

                <TextView
                    android:id="@+id/txt_drc_evidence"
                    style="@style/CustomTextViewPadding5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1.4"
                    android:fontFamily="@font/montserrat_regular"
                    android:text="GRN Evidence"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_10ssp" />

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_2sdp"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_weight="1"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="@dimen/_3sdp"
                    app:cardElevation="@dimen/_2sdp">

                    <TextView
                        android:id="@+id/selectButtonGrndealer"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:background="@color/Green"
                        android:fontFamily="@font/montserrat_medium"
                        android:gravity="center"
                        android:text="Choose File"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="18sp" />

                </androidx.cardview.widget.CardView>


            </LinearLayout>


            <!--GRN Evidence-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="120dp"
                android:layout_marginTop="20dp"
                android:orientation="vertical"

                android:visibility="gone">


                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="@dimen/_3sdp"
                    app:cardElevation="@dimen/_2sdp">

                    <TextView
                        android:id="@+id/fileDisplay1"
                        style="@style/CustomTextViewPadding5dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1.4"
                        android:fontFamily="@font/montserrat_regular"
                        android:text="Selected File:"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_12ssp"
                        android:visibility="gone" />

                    <ImageView
                        android:id="@+id/imgGrnProEv"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/upload_doc" />

                </androidx.cardview.widget.CardView>
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_15ssp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txt_word_evidence_document"
                    style="@style/CustomTextViewPadding5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1.4"
                    android:fontFamily="@font/montserrat_regular"
                    android:gravity="center"
                    android:text="Evidence Document"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_10ssp" />

                <ImageView
                    android:id="@+id/imgGrnDelEv"
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="@dimen/_80sdp"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/_10ssp"
                    android:layout_weight="1"
                    android:src="@drawable/upload_doc" />

            </LinearLayout>


            <!--GRN Evidence-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="120dp"
                android:layout_marginTop="20dp"
                android:orientation="vertical"
                android:visibility="gone">


                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="@dimen/_3sdp"
                    app:cardElevation="@dimen/_2sdp">

                    <TextView
                        android:id="@+id/fileDisplay"
                        style="@style/CustomTextViewPadding5dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1.4"
                        android:fontFamily="@font/montserrat_regular"
                        android:text="Selected File:"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_12ssp"
                        android:visibility="gone" />
                </androidx.cardview.widget.CardView>
            </LinearLayout>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="30dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true">

                    <TextView
                        android:id="@+id/txt_save_grn_data"
                        android:layout_width="@dimen/_110sdp"
                        android:layout_height="@dimen/_30ssp"
                        android:background="@drawable/rounded_button"
                        android:fontFamily="@font/montserrat_medium"
                        android:gravity="center"
                        android:text="@string/save"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="18sp" />
                </LinearLayout>
            </RelativeLayout>

        </LinearLayout>


    </LinearLayout>

</LinearLayout>